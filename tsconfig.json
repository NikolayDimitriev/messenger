{
	"compilerOptions": {
		"outDir": "dist",
		"target": "es2016",
		"declaration": false,
		"module": "commonjs",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"sourceMap": true,
		"strictNullChecks": false
	},
	"include": ["index.d.ts", "images.d.ts", "src/**/*.ts"],
	"exclude": ["node_modules", "dist"]
}
